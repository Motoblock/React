{
  "name": "task6",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "start": "cross-env NODE_ENV=production && ts-node ./server",
    "dev:client": "npm run build:client && vite --config vite.config.ts dev",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build": "tsc --noEmit -p tsconfig.prod.json && npm run build:client && npm run build:server && npm run copy-files",
    "copy-files": "copyfiles static/* dist/assets && copyfiles index.html dist && copyfiles -f dist/client/assets/* dist/assets",
    "lint": "eslint . --ext .tsx --ext .ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:clean": "npx rimraf coverage && npx rimraf .nyc_output",
    "cypress": "cypress open",
    "cypress:screen": "npx cypress run --e2e && npx nyc report --reporter=text-summary",
    "ci": "start-server-and-test dev http://localhost:3000 cypress:screen"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "netlify": "^13.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "redux-mock-store": "^1.5.4",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/coverage-istanbul": "^0.30.1",
    "compression": "^1.7.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.10.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "react-hook-form": "^7.43.8",
    "react-lorem-ipsum": "^1.4.10",
    "rimraf": "^5.0.0",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "^4.2.3",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.30.1"
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  }
